<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.0;net35</TargetFrameworks>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>GON.TlvExtensions</Title>
    <FileVersion>1.1.0.0</FileVersion>
    <AssemblyVersion>1.1.0.0</AssemblyVersion>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/god-of-north/TlvExtensions</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>BER BERTLV TLV EMV SmartCard</PackageTags>
    <PackageProjectUrl>https://github.com/god-of-north/TlvExtensions</PackageProjectUrl>
    <Description>.Net Extenstions to simplify work with SIMPLE-TLV and BER-TLV arrays</Description>
    <PackageId>GON.TlvExtensions</PackageId>
    <Version>1.1.0</Version>
    <Authors>Maksym Lesyk</Authors>
    <Product>GON.TlvExtensions</Product>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="GON.HexadecimalStringExtensions" Version="1.2.0" />
  </ItemGroup>

</Project>
